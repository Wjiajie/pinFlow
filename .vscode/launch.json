        {
            "name": "pinFlow (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "pinFlow (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
